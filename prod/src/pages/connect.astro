---
import BaseLayout from '../layouts/BaseLayout.astro';
import Connect from '../components/connect/Connect.astro';
import connectData from '../data/connect.json';

const FORMSPREE_ID = 'mlgednnn';
---

<BaseLayout title="Connect - Mike Lacey">
  <Connect
    contactInfo={connectData.contactInfo}
    professionalLinks={connectData.professionalLinks}
    formFields={connectData.formFields}
    sectionContent={connectData.sectionContent}
    formspreeId={FORMSPREE_ID}
  />
</BaseLayout>
